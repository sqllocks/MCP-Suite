{
  "client_id": "client-fabric",
  "connections": {
    "fabric-warehouse": {
      "name": "Production Fabric Warehouse",
      "type": "fabric-warehouse",
      "host": "contoso-workspace.datawarehouse.fabric.microsoft.com",
      "database": "SalesWarehouse",
      "read_only": true,
      "timeout": 30,
      "options": {
        "authentication": {
          "type": "azure-active-directory-default"
        },
        "encrypt": true
      }
    },
    "fabric-sql-database": {
      "name": "Analytics SQL Database",
      "type": "fabric-sql-database",
      "host": "contoso-workspace.datawarehouse.fabric.microsoft.com",
      "database": "AnalyticsDB",
      "read_only": true,
      "timeout": 30,
      "options": {
        "authentication": {
          "type": "azure-active-directory-default"
        },
        "encrypt": true
      }
    },
    "fabric-lakehouse": {
      "name": "Data Lakehouse",
      "type": "fabric-lakehouse",
      "host": "contoso-workspace.datawarehouse.fabric.microsoft.com",
      "database": "DataLakehouse",
      "read_only": true,
      "timeout": 30,
      "options": {
        "authentication": {
          "type": "azure-active-directory-default"
        },
        "encrypt": true
      }
    }
  },
  "max_result_rows": 1000,
  "query_timeout": 30,
  "enable_explain_plan": true,
  "allow_write_operations": false
}
